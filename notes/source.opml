						<outline text="Here's a script in my root.opml file." created="Mon, 10 May 2021 23:51:27 GMT">
							<outline text="getFriendsInfo" created="Mon, 10 May 2021 23:52:04 GMT">
								<outline text="function () {" created="Mon, 10 May 2021 23:52:08 GMT">
									<outline text="var theArray = new Array ();"/>
									<outline text="var theProfile = twitter.getUserProfile (&quot;davewiner&quot;);" created="Tue, 11 May 2021 02:08:31 GMT"/>
									<outline text="console.log (jsonStringify (theProfile))"/>
