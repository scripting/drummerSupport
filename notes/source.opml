				<outline text="May 10" created="Mon, 10 May 2021 15:38:02 GMT" name="10" type="calendarDay">
					<outline text="[[concordScript]]" created="Mon, 10 May 2021 23:51:18 GMT" type="outline">
						<outline text="Here's a script in my root.opml file." created="Mon, 10 May 2021 23:51:27 GMT">
							<outline text="getFriendsInfo" created="Mon, 10 May 2021 23:52:04 GMT">
								<outline text="function () {" created="Mon, 10 May 2021 23:52:08 GMT">
									<outline text="var theArray = new Array ();"/>
									<outline text="var theProfile = twitter.getUserProfile (&quot;davewiner&quot;);" created="Tue, 11 May 2021 02:08:31 GMT"/>
									<outline text="console.log (jsonStringify (theProfile))"/>
									<outline text="theArray.push (theProfile);"/>
									<outline text="return (theArray);"/>
									<outline text="}"/>
									</outline>
								</outline>
							</outline>
						<outline text="I call it from a script that I run from an outline." created="Mon, 10 May 2021 23:52:18 GMT">
							<outline text="root.getFriendsInfo ()" created="Mon, 10 May 2021 23:52:32 GMT">
								<outline text="[" isComment="true" created="Mon, 10 May 2021 23:53:03 GMT">
									<outline text="{ }" created="Mon, 10 May 2021 23:53:13 GMT"/>
									<outline text="]"/>
									</outline>
								</outline>
							</outline>
