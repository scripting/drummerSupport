	<body created="Tue, 01 Jun 2021 12:50:24 GMT" name="june2021" text="notes/2021/06.mdo" type="calendarMonth">
		<outline created="Tue, 29 Jun 2021 15:31:31 GMT" name="29" text="June 29" type="calendarDay">
			<outline created="Wed, 30 Jun 2021 02:11:34 GMT" text="when processing one of the twitter outlines" type="outline">
				<outline created="Wed, 30 Jun 2021 02:11:46 GMT" text="look for t.co urls and replace them with a link node"></outline>
				</outline>
			<outline created="Wed, 30 Jun 2021 02:06:09 GMT" text="for tomorrow --" type="outline">
				<outline created="Wed, 30 Jun 2021 02:06:13 GMT" text="review publish and subscribe commands in the file menu">
					<outline created="Wed, 30 Jun 2021 02:06:25 GMT" text="it should be as simple as &quot;make this file public&quot; (if it's private)"></outline>
					<outline created="Wed, 30 Jun 2021 02:06:43 GMT" text="and &quot;subscribe to outline&quot; and you enter the url for the OPML file">
						<outline created="Wed, 30 Jun 2021 02:06:58 GMT" text="you get an error if it doesn't have the right headers"></outline>
						</outline>
					</outline>
				<outline created="Wed, 30 Jun 2021 02:17:25 GMT" text="in drummer no need to call instantoutliner.com">
