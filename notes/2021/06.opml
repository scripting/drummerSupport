		<outline created="Mon, 14 Jun 2021 13:45:04 GMT" name="14" text="June 14" type="calendarDay">
			<outline created="Mon, 14 Jun 2021 13:45:05 GMT" text="[[tagserver]]" type="outline">
				<outline created="Mon, 14 Jun 2021 13:45:43 GMT" text="plan">
					<outline created="Mon, 14 Jun 2021 14:13:44 GMT" text="start a new server just for scripting.com">
						<outline created="Mon, 14 Jun 2021 14:13:52 GMT" text="tags.scripting.com"></outline>
						</outline>
					<outline created="Mon, 14 Jun 2021 14:19:20 GMT" text="the database is named">
						<outline created="Mon, 14 Jun 2021 14:19:27 GMT" text="scriptingtagbase"></outline>
						</outline>
					<outline created="Mon, 14 Jun 2021 14:15:35 GMT" text="in oldschool, when rendering a tag for say Bull Mancuso, replace it with a link like this">
						<outline created="Mon, 14 Jun 2021 14:16:11 GMT" text="[[&lt;a href=&quot;http://tags.scripting.com/?tag=Bull Mancuso&quot;&gt;Bull Mancuso&lt;/a&gt;]]"></outline>
						</outline>
					<outline created="Mon, 14 Jun 2021 14:17:55 GMT" text="When you go to that page, it shows a reverse chronologic table of references to the term, with the actual text indented underneath."></outline>
					</outline>
				<outline created="Mon, 14 Jun 2021 13:45:43 GMT" text="new http endpoint -- /gettagrefs?tag=x">
					<outline created="Mon, 14 Jun 2021 13:46:11 GMT" text="returns an array of nodes that refernce the indicated tag"></outline>
					</outline>
				<outline created="Mon, 14 Jun 2021 13:45:11 GMT" text="I've got it running on my local machine">
					<outline created="Mon, 14 Jun 2021 13:45:18 GMT" text="http://localhost:1491/"></outline>
					</outline>
				</outline>
			</outline>
		<outline created="Sun, 13 Jun 2021 15:14:50 GMT" name="13" text="June 13" type="calendarDay">
			<outline created="Sun, 13 Jun 2021 15:14:51 GMT" text="[[tagserver]] doesn't have to do as much as it does" type="outline">
				<outline created="Sun, 13 Jun 2021 15:16:20 GMT" text="this is what's required">
					<outline created="Sun, 13 Jun 2021 15:16:24 GMT" text="submit an outline and add each of its tags to the database"></outline>
					<outline created="Sun, 13 Jun 2021 15:16:47 GMT" text="ask for all instances of that tag, in reverse chronologic order"></outline>
					</outline>
				</outline>
			</outline>
		<outline created="Sat, 12 Jun 2021 21:01:04 GMT" name="12" text="June 12" type="calendarDay">
			<outline created="Sat, 12 Jun 2021 21:01:05 GMT" text="Ideas for next directions" type="outline">
				<outline created="Sat, 12 Jun 2021 21:02:49 GMT" text="release drummer alpha?">
					<outline created="Sat, 12 Jun 2021 21:02:57 GMT" text="this might be the time"></outline>
					<outline created="Sat, 12 Jun 2021 21:03:07 GMT" text="i don't want to update LO2"></outline>
					<outline created="Sun, 13 Jun 2021 02:31:15 GMT" text="i want a tool that works with tagserver (see below)"></outline>
					</outline>
				<outline created="Sat, 12 Jun 2021 21:02:44 GMT" text="github verbs for drummer"></outline>
				<outline created="Sat, 12 Jun 2021 21:01:10 GMT" text="tagserver">
					<outline created="Sat, 12 Jun 2021 21:02:14 GMT" text="fresh database"></outline>
					<outline created="Sat, 12 Jun 2021 21:02:32 GMT" text="docs/announcement"></outline>
					<outline created="Sat, 12 Jun 2021 21:02:36 GMT" text="open source release?"></outline>
					</outline>
				<outline created="Sat, 12 Jun 2021 21:01:15 GMT" text="instantoutliner">
					<outline created="Sat, 12 Jun 2021 21:01:21 GMT" text="use a relational database"></outline>
					<outline created="Sat, 12 Jun 2021 21:01:26 GMT" text="use a signon"></outline>
